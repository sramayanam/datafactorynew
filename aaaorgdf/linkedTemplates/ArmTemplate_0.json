							"notebookPath": "/Users/srram@dataaidays.net/createsparktableforreporting",
							"baseParameters": {
								"filename": {
									"value": "@pipeline().parameters.copyFile",
									"type": "Expression"
								},
								"filepath": {
									"value": "@pipeline().parameters.copyFolder",
									"type": "Expression"
								}
